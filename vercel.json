{
  "builds": [
    {
      "src": "Software_Engineering___Agile_Web_Application/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.6" }
    }
  ],
  "routes": [
    {
      "src": "/app/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Software_Engineering___Agile_Web_Application/wsgi.py"
    }
  ]
}
